pipeline {
agent any 
stages
{
stage{'Build'}
{
steps{
echo "Building the code....."
sh "mvn clean"
}
}
stage{'Test'}
{
steps{
echo "Test the code....."
sh "mvn test"
}
}
stage{'Compile'}
{
steps{
echo "compile the code..."
sh "mvn compile"
}
}
stage{'package'}
{
steps{
echo "package the code..."
sh "mvn package"
}
}
stage{'Deplo'}
{
steps{
echo "deploy the code..."
sh "mvn deploy"
}
}
}
}


